[alias]
  s = status -sb
  co = checkout
  br = branch
  cm = commit -m
  ca = commit --amend
  cam = commit --amend -m
  cane = commit --amend --no-edit
  df = diff
  dc = diff --cached
  lg = log --oneline --graph --decorate --all
  last = log -1 HEAD
  undo = reset --soft HEAD~1
  rst = reset --hard
  stash-pop = stash pop
  stash-list = stash list
  stash-clear = stash clear
  unstage = reset HEAD --
  uncommit = reset --soft HEAD~1
  rbi = rebase -i
  fixup = commit --fixup
  squash = rebase -i --autosquash
  fpush = push --force-with-lease
  fp = pull --rebase
  cleanup = "!git branch --merged | grep -v '*' | xargs git branch -d"
